{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "C/C++: gcc build active file",
			"command": "/sbin/gcc",
			"args": [
				"-lOpenCL",
				"-lm",
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "/sbin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}